#!/bin/sh -e

case "$1" in
	remove)
		# Update PAM configurations
		pam-auth-update --package --remove mauth
	;;

	upgrade|deconfigure|failed-upgrade)
	;;


	*)
		printf "prerm called with unknown argument '$1'\n" >&2
		exit 1
	;;
esac


# dh_installdeb will replace this with shell code automatically generated by
# other debhelper scripts. see: dh_installdeb(1)

#DEBHELPER#


exit 0
